programa
{	
	
	funcao inicio()
	{
		inteiro idade 
		escreva("\nQual a sua idade? ")
		leia(idade)

			se (idade>=5 e idade<=7)
			{		
				escreva("\nVocê está na categoria Infantil A! ")
			}			
			senao se (idade>=8 e idade<=11)
			{
				escreva("\nVocê está na categoria Infantil B! ")
			}
			senao se (idade>=12 e idade<=13)
			{
				escreva("\nVocê está na categoria Infantil C!" )
			}

			senao se (idade>=14 e idade<=17)
			{
				escreva("\nVocê está na categoria Infantil D!" )
			}

			senao se (idade>=18)
			{
				escreva("\nVocê está na categoria Infantil Adulto! ")
			}
		
	}

}

